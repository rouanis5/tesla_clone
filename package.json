{
  "name": "tesla_clone",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "gh-pages": "gh-pages -d dist",
    "deploy": "yarn build --base=./ && yarn gh-pages",
    "test:format": "prettier --check .",
